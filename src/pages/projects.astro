---
import { Image } from 'astro:assets';
import BaseLayout from '../layouts/BaseLayout.astro';
import Database from '../components/icons/Database.astro';
import Emergence from '../components/icons/Emergence.astro';
import Target from '../components/icons/Target.astro';
import AIResearch from '../components/icons/AIResearch.astro';
import Operations from '../components/icons/Operations.astro';
import Consciousness from '../components/icons/Consciousness.astro';
import projectsBg from '../assets/images/projects-bg.jpg';
---

<BaseLayout
  title="Projects - Ashwin Philips"
  description="Production systems and research projects by Ashwin Philips."
  pageKey="projects"
>
  <!-- Hero -->
  <section class="relative overflow-hidden bg-gray-900">
    <div class="absolute inset-0">
      <Image
        src={projectsBg}
        alt=""
        width={1920}
        height={1080}
        format="webp"
        quality={60}
        loading="lazy"
        class="w-full h-full object-cover opacity-40 parallax-slow ken-burns-d"
        data-parallax-speed="0.3"
      />
      <div class="absolute inset-0 bg-gradient-to-b from-gray-900 via-gray-900/95 to-gray-900"></div>
    </div>
    <div class="relative container pt-32 pb-20 z-10">
      <h1 class="text-5xl md:text-6xl font-extrabold mb-6 text-white">Projects</h1>
      <p class="text-xl md:text-2xl font-light text-gray-300 max-w-3xl">
        Production-scale systems and research implementations exploring distributed
        intelligence, computational phenomenology frameworks, and autonomous infrastructure.
      </p>
    </div>
  </section>

  <!-- Featured Projects -->
  <section class="bg-white dark:bg-gray-900">
    <div class="container">
      <h2 class="text-3xl font-bold mb-8 text-gray-900 dark:text-white">Featured Projects</h2>
      <div class="max-w-5xl mx-auto space-y-12">

        <!-- qDB -->
        <div id="qdb" class="scroll-mt-20 fade-in-on-scroll card-parallax" data-fade-delay="0">
          <div class="p-8 bg-white dark:bg-gray-800 rounded-lg border border-gray-200 dark:border-gray-700">
            <div class="flex items-start gap-6">
              <Database class="w-16 h-16 text-primary-600 dark:text-primary-400" />
              <div class="flex-1">
                <div class="flex items-center gap-3 mb-3">
                  <h2 class="text-3xl font-bold text-gray-900 dark:text-white">qDB</h2>
                  <span class="px-3 py-1 bg-primary-100 text-primary-800 text-xs font-medium rounded-full dark:bg-primary-900 dark:text-primary-300 uppercase">Production</span>
                </div>
                <p class="text-xl text-primary-600 dark:text-primary-500 mb-4 font-medium">
                  Algorithmic compression achieving 95%+ data reduction
                </p>
                <p class="text-gray-600 dark:text-gray-300 mb-6">
                  Novel approach to data compression and retrieval. Provides users with truth discovery
                  and emergence pattern recognition through compressed semantic indexing. Enables
                  pattern-based retrieval without decompression.
                </p>

                <h3 class="text-lg font-bold mb-3 text-gray-900 dark:text-white">Performance Metrics</h3>
                <ul class="space-y-2 mb-6">
                  <li class="flex items-start gap-2 text-gray-600 dark:text-gray-300">
                    <span class="text-primary-600 dark:text-primary-500 mt-1">‚Üí</span>
                    <strong class="text-gray-900 dark:text-white">&lt; 100ms</strong> query latency at scale
                  </li>
                  <li class="flex items-start gap-2 text-gray-600 dark:text-gray-300">
                    <span class="text-primary-600 dark:text-primary-500 mt-1">‚Üí</span>
                    <strong class="text-gray-900 dark:text-white">2PB dataset ‚Üí 100TB</strong> compressed (95%+ reduction)
                  </li>
                  <li class="flex items-start gap-2 text-gray-600 dark:text-gray-300">
                    <span class="text-primary-600 dark:text-primary-500 mt-1">‚Üí</span>
                    Pattern-based retrieval without decompression overhead
                  </li>
                  <li class="flex items-start gap-2 text-gray-600 dark:text-gray-300">
                    <span class="text-primary-600 dark:text-primary-500 mt-1">‚Üí</span>
